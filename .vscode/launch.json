    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": ".",            
            "args": ["-add" ,"Foo Add item from debug"],
            "env": {"APP_ENV": "development"
            }
        },
        {
            "name": "Launch file debug server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": ".",            
            "args": ["-runserver"],
            "env": {"APP_ENV": "development"
            }
        },        
        {
            "name": "Launch file run",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": ".", 
            "args": ["-add", "\"Lorem ipsum dolor sit amet, consectetur adipiscing elit\""]
        }
    ]
}